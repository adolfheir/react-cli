{
  "parser": "babel-eslint",
  "extends": "eslint-config-ali/react",
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "max-len": "off",
    "react/prop-types": 0,
    "react/forbid-prop-types": [
      1,
      {
        "forbid": [
          "any"
        ]
      }
    ]
  }
}
